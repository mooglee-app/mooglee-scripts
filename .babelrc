{
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": [
        ["next/babel", {
          "preset-env": {
            "targets": {
              "browsers": ["last 2 versions", "ios >= 10"]
            }
          }
        }]
      ],
      "plugins": []
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}